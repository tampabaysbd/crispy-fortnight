<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - QDI</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("styles", required: false)
</head>
<body>
    <header>
        <div class="branding">
            <strong>Contact Keeper Inc. Contacts Code Challenge</strong> 
        </div>
    </header>
    <article>
        <div id="breadcrum">
            @Html.ActionLink("Return to Home", "Index", "Home")
        </div>

        @RenderBody()
    </article>
    <footer>
        <small>&copy; @DateTime.Now.Year Contact Keeper Inc.</small>
    </footer>
    @RenderSection("breakout", required: false)
    @Scripts.Render("~/bundles/site")
    @RenderSection("scripts", required: false)
</body>
</html>
